<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="376.0" prefWidth="923.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.ControllerInventory">
   <children>
      <TableView fx:id="table" layoutX="10.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="col1" prefWidth="75.0" text="id" />
            <TableColumn fx:id="col2" prefWidth="420.0" text="Название" />
            <TableColumn fx:id="col3" minWidth="0.0" prefWidth="407.0" text="Тип" />
        </columns>
      </TableView>
      <TextField fx:id="id" layoutX="14.0" layoutY="237.0" prefHeight="31.0" prefWidth="51.0" promptText="id" />
      <TextField fx:id="name" layoutX="99.0" layoutY="237.0" prefHeight="31.0" prefWidth="150.0" promptText="Название" />
      <ChoiceBox fx:id="type" layoutX="297.0" layoutY="237.0" prefWidth="150.0" />
      <Button layoutX="504.0" layoutY="237.0" mnemonicParsing="false" onAction="#search" text="Найти" />
      <Button layoutX="504.0" layoutY="275.0" mnemonicParsing="false" onAction="#delete" text="Удалить" />
      <Button layoutX="504.0" layoutY="319.0" mnemonicParsing="false" onAction="#save" text="Сохранить" />
   </children>
</AnchorPane>
